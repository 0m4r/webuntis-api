{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "docusaurus",
  "projectType": "application",
  "root": "apps/docusaurus",
  "sourceRoot": "apps/docusaurus/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/build"],
      "dependsOn": ["^build"],
      "options": {
        "command": "npm run build --workspace apps/docusaurus"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run start --workspace apps/docusaurus"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "command": "npm run deploy --workspace apps/docusaurus"
      }
    }
  },
  "tags": []
}
