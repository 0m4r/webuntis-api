{
  "name": "@0m4r/webuntis-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "WebUntis monorepo workspace",
  "license": "MIT",
  "packageManager": "yarn@4.12.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/0m4r/webuntis-api.git"
  },
  "devDependencies": {
    "@nx/js": "19.8.0",
    "@nx/workspace": "19.8.0",
    "@types/node": "^22",
    "dotenv": "^17.2.3",
    "nx": "19.8.0",
    "prettier": "^3.7.4"
  },
  "scripts": {
    "nx": "nx",
    "build": "nx run webuntis-api:build",
    "test": "nx run webuntis-api:test",
    "docs": "nx run-many -t typedoc,build --projects=apps/docusaurus",
    "docs:dev": "nx run docusaurus:start",
    "docs:deploy": "nx run docusaurus:deploy",
    "examples": "nx run examples:build",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release:webuntis-api": "node scripts/release-webuntis-api.js"
  }
}
