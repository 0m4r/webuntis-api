  },
  "devDependencies": {
    "@types/node": "^22",
    "esbuild": "^0.27.1",
    "jest": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-in-case": "^1.0.2",
    "prettier": "^3.7.4",
    "rollup": "^4.53.4",
