{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "webuntis-api",
  "projectType": "library",
  "root": "packages/webuntis-api",
  "sourceRoot": "packages/webuntis-api/src",
  "targets": {
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn --cwd packages/webuntis-api prebuild"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["prebuild"],
      "options": {
        "command": "yarn --cwd packages/webuntis-api build"
      }
    },
    "postbuild": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["build"],
      "options": {
        "command": "yarn --cwd packages/webuntis-api postbuild"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/coverage"],
      "dependsOn": ["build"],
      "options": {
        "command": "yarn --cwd packages/webuntis-api test"
      }
    }
  },
  "tags": []
}
